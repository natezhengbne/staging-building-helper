{
    "manifest_version": 3,
    "name": "Staging server building helper",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "cookies",
        "tabs",
        "scripting",
        "activeTab"
    ]
}