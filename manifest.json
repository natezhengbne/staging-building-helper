{
    "manifest_version": 3,
    "name": "Staging server building helper",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon128.png",
        "32": "icon128.png",
        "48": "icon128.png",
        "128": "icon128.png"
    },
    "host_permissions": [
        "https://*.dev.benon.com/*",
        "http://*.dev.benon.com/*"
    ],
    "permissions": [
        "cookies",
        "tabs",
        "scripting",
        "activeTab"
    ]
}